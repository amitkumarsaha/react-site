{
  "name": "inventory-site",
  "version": "1.0.0",
  "description": "Starter site for inventory operations built using React and Redux",
  "author": "Amit Joshi",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node tools/start.message.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/src.server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/test.setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist",
    "clean-css": "npm run remove-css",
    "clean-html": "npm run remove-html",
    "remove-dist": "rimraf ../inventory-service/src/main/resources/static/js/*.js",
    "remove-css": "rimraf ../inventory-service/src/main/resources/static/*.css",
    "remove-html": "rimraf ../inventory-service/src/main/resources/templates/index.html",
    "build:html": "babel-node tools/build.html.js",
    "prebuild": "npm-run-all clean-dist clean-css clean-html test lint build:html",
    "build": "babel-node tools/build.js",
    "buld:watch": "npm run start:dev build -- --watch"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "history": "^4.10.1",
    "isomorphic-fetch": "^2.2.1",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bubble-preloader": "^1.3.2",
    "react-dom": "^16.13.1",
    "react-json-pretty": "^2.2.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "4.0.8",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@hapi/hoek": "^9.0.4",
    "acorn": "^7.1.1",
    "acorn-jsx": "^5.2.0",
    "argparse": "^1.0.10",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.4.1",
    "cheerio": "0.22.0",
    "colors": "1.4.0",
    "compression": "^1.7.4",
    "cross-env": "6.0.3",
    "css-loader": "^3.5.1",
    "dot": "^1.1.3",
    "enzyme": "3.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-watch": "^6.0.1",
    "eventsource-polyfill": "0.9.6",
    "expect": "24.9.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "5.0.2",
    "gensync": "^1.0.0-beta.1",
    "hoek": "^6.1.3",
    "jquery": "^3.4.1",
    "jsdom": "^16.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "nock": "11.7.2",
    "npm-run-all": "4.1.5",
    "open": "^7.0.3",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
